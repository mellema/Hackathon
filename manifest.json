{
  "name": "GroupedIn",
  "version": "0.0.1",
  "manifest_version": 2,
  "description" : "A Chrome extension that allows faster search for LinkedIn group members ",
  "background": {
  "scripts": ["background.js"],
  "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/groups*"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": {
        "19": "19x19.gif"
    },
    "default_title": "That's the tool tip",
    "default_popup": "popup.html"
  }
}